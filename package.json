{
  "name": "illumination",
  "version": "0.0.1",
  "description": "Simple, powerful Philips Hue client.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha src/**/*test.js --opts mocha.opts",
    "lint": "eslint src/",
    "babel": "babel src/ -d dist/ --ignore **/*test.js",
